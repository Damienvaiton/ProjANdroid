<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    >


    <TextView
        android:id="@+id/nomPoint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp"
        android:text="PAIGESTO0000000320306501"
        android:textSize="12sp"


        />


    <TextView
        android:id="@+id/typeEau"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp"
        android:layout_toEndOf="@id/nomPoint"
        android:text="NomStation"
        android:textSize="11sp"


        />

    <TextView
        android:id="@+id/NomCommune"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp"
        android:layout_toEndOf="@id/typeEau"
        android:text="NomCommune"
        android:textSize="10sp"



        />

</RelativeLayout>